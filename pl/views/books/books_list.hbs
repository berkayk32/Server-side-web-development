{{> pagination}}

{{#each books}}
    <div id="book_info">
        <a href="/books/{{ISBN}}">

            <div>ISBN: {{ISBN}}</div>
            <div>Title: {{title}}</div>
            <div>publicationYear: {{publicationYear}}</div>
            <div>Pages: {{pages}}</div>
        </a>
    </div>
    <form action="books/delete/{{ISBN}}" id="deleteForm" method="post">
        <input type="submit" value="Delete"/>
    </form>
    
{{/each}}

{{> pagination}}
