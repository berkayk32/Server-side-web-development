
{{#each books}}
    <div id="book_info">
        <a href="/books/{{ISBN}}">

            <div>ISBN: {{ISBN}}</div>
            <div>Title: {{title}}</div>
            <div>publicationYear: {{publicationYear}}</div>
            <div>Pages: {{pages}}</div>
        </a>
    </div>
{{/each}}

{{#each books.pages}}
    <a href="/books?page={{this.value}}">
        <input type="button" value="{{this.value}}">
    </a>
{{/each}}